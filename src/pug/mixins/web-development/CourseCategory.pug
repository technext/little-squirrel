mixin Courses(config)
  .col-md-4.mb-4
    .card.h-100
      img(src=`${CWD}assets/img/gallery/${config.image}` alt='courses')
      .card-body
        h5.font-sans-serif.fw-bold!=config.title
        p.text-muted.fs--1!=config.text

mixin CourseCategory
  +Section.pb-0(style="margin-top:-17rem") 
    .row
      +Courses(category[0])
      +Courses(category[1])
      +Courses(category[2])
      +Courses(category[3])
      +Courses(category[4])
