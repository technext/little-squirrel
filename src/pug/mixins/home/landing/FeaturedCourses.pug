mixin Courses(config)
  .col-md-4.mb-4
    .card.h-100
      img.card-img-top.w-100(src=`${CWD}assets/img/gallery/${config.image}` alt='courses')
      .card-body
        h5.font-sans-serif.fw-bold.fs-md-0.fs-lg-1!=config.title
        a.text-muted.fs--1.stretched-link(href='#!')!=config.text
          
mixin FeaturedCourses 
  +Section 
    .row 
      h1.text-center.mb-5  Top Featured Courses
      +Courses(courses[0])
      +Courses(courses[1])
      +Courses(courses[2])
      +Courses(courses[3])
      +Courses(courses[4])
      +Courses(courses[5])
